{
    "name": "pcbmf",
    "productName": "PCB Mini Factory",
    "version": "0.0.1",
    "description": "A Minimal Software for Milling and Drilling PCB with simple CNC",
    "main": ".webpack/main",
    "repository": {
        "url": "git@github.com:dianlight/pcbmf.git",
        "type": "git"
    },
    "scripts": {
        "start": "electron-forge start",
        "package": "electron-forge package",
        "make": "electron-forge make",
        "publish": "electron-forge publish",
        "lint": "eslint --ext .ts ."
    },
    "keywords": [],
    "author": {
        "name": "dianlight",
        "email": "lucio.tarantino@gmail.com"
    },
    "license": "MIT",
    "config": {
        "forge": {
            "packagerConfig": {},
            "publishers": [
                {
                    "name": "@electron-forge/publisher-github",
                    "config": {
                        "repository": {
                            "owner": "dianlight",
                            "name": "pcbmf"
                        },
                        "prerelease": true
                    }
                }
            ],
            "makers": [
                {
                    "name": "@electron-forge/maker-squirrel",
                    "config": {
                        "name": "my_new_app"
                    }
                },
                {
                    "name": "@electron-forge/maker-zip",
                    "platforms": [
                        "darwin"
                    ]
                },
                {
                    "name": "@electron-forge/maker-deb",
                    "config": {}
                },
                {
                    "name": "@electron-forge/maker-rpm",
                    "config": {}
                }
            ],
            "plugins": [
                [
                    "@electron-forge/plugin-webpack",
                    {
                        "mainConfig": "./webpack.main.config.js",
                        "renderer": {
                            "config": "./webpack.renderer.config.js",
                            "entryPoints": [
                                {
                                    "html": "./src/index.html",
                                    "js": "./src/renderer.ts",
                                    "name": "main_window"
                                }
                            ]
                        }
                    }
                ]
            ]
        }
    },
    "devDependencies": {
        "@electron-forge/cli": "^6.0.0-beta.54",
        "@electron-forge/maker-deb": "^6.0.0-beta.54",
        "@electron-forge/maker-rpm": "^6.0.0-beta.54",
        "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
        "@electron-forge/maker-zip": "^6.0.0-beta.54",
        "@electron-forge/plugin-auto-unpack-natives": "^6.0.0-beta.54",
        "@electron-forge/plugin-webpack": "^6.0.0-beta.54",
        "@electron-forge/publisher-github": "^6.0.0-beta.54",
        "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
        "@types/adm-zip": "^0.4.33",
        "@types/serialport": "^8.0.1",
        "@types/yaml": "^1.9.7",
        "@typescript-eslint/eslint-plugin": "^4.8.2",
        "@typescript-eslint/parser": "^4.8.2",
        "copy-webpack-plugin": "^6.3.2",
        "css-loader": "^4.3.0",
        "electron": "^11.0.3",
        "eslint": "^7.14.0",
        "eslint-plugin-import": "^2.22.1",
        "file-loader": "^6.2.0",
        "fork-ts-checker-webpack-plugin": "^5.2.1",
        "node-loader": "^1.0.2",
        "style-loader": "^1.3.0",
        "ts-loader": "^8.0.11",
        "tsconfig-paths-webpack-plugin": "^3.3.0",
        "typescript": "^4.1.2",
        "url-loader": "^4.1.1",
        "vue-devtools": "^5.1.4",
        "vue-loader": "^15.9.5",
        "vue-template-compiler": "^2.6.12"
    },
    "dependencies": {
        "@ncform/ncform": "^1.7.2",
        "@ncform/ncform-common": "^1.7.2",
        "@ncform/ncform-theme-elementui": "^1.7.2",
        "adm-zip": "^0.5.1",
        "axios": "^0.18.1",
        "conf": "^7.1.2",
        "electron-log": "^4.3.0",
        "electron-squirrel-startup": "^1.0.0",
        "electron-store": "^6.0.1",
        "element-ui": "^2.14.1",
        "pcb-stackup": "^4.2.2",
        "serialport": "^9.0.2",
        "tightcnc": "^0.2.0",
        "update-electron-app": "^2.0.1",
        "vue": "^2.6.12",
        "vue-class-component": "^7.2.6",
        "vue-property-decorator": "^9.1.2",
        "vue-router": "^3.4.9",
        "vuex": "^3.6.0",
        "vuex-map-fields": "^1.4.1",
        "vuex-persist": "^3.1.3",
        "yaml": "^1.10.0"
    }
}
