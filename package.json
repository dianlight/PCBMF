{
    "name": "pcbmf",
    "productName": "PCB Mini Factory",
    "version": "0.0.1-alpha",
    "private": true,
    "description": "A Minimal Software for Milling and Drilling PCB with simple CNC",
    "homepage": "https://github.com/dianlight/PCBMF#readme",
    "main": ".webpack/main",
    "repository": {
        "url": "git@github.com:dianlight/PCBMF.git",
        "type": "git"
    },
    "bugs": {
        "url": "https://github.com/dianlight/PCBMF/issues"
    },
    "engines": {
        "node": ">=13.0.0 <= 14.0.0"
    },
    "scripts": {
        "start": "concurrently -k \"electron-forge start\" \"npm:watch\"",
        "package": "electron-forge package",
        "make": "electron-forge make",
        "publish": "electron-forge publish",
        "lint": "eslint --ext .ts .",
        "postinstall": "patch-package --exclude && husky install && electron-rebuild",
        "generate": "json2ts src/vue/pages/schemas/ -o src/typings",
        "prestart": "npm run generate",
        "watch": "npm-watch",
        "licence": "electron-license list --json --production",
        "test": "npm run package",
        "commit": "git-cz",
        "gen:patch": "patch-package --exclude '' gcode-toolpath vuex-map-fields @types/jsts jsts",
        "release:major": "changelog -M && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version major && git push origin && git push origin --tags",
        "release:minor": "changelog -m && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version minor && git push origin && git push origin --tags",
        "release:patch": "changelog -p && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version patch && git push origin && git push origin --tags"
    },
    "keywords": [
        "CNC",
        "PCB"
    ],
    "author": {
        "name": "dianlight",
        "email": "lucio.tarantino@gmail.com"
    },
    "license": "GPL3",
    "watch": {
        "generate": "src/vue/pages/schemas/*.json"
    },
    "asarUnpack": [
        ".webpack/main/0.bundle.worker.js",
        ".webpack/main/0.bundle.worker.js.map"
    ],
    "config": {
        "forge": {
            "packagerConfig": {},
            "publishers": [{
                "name": "@electron-forge/publisher-github",
                "config": {
                    "repository": {
                        "owner": "dianlight",
                        "name": "pcbmf"
                    },
                    "prerelease": true
                }
            }],
            "makers": [{
                    "name": "@electron-forge/maker-squirrel",
                    "config": {
                        "setupExe": "${npm_package_name}-${npm_package_arch}-${npm_package_version}.exe"
                    }
                },
                {
                    "name": "@electron-forge/maker-zip",
                    "platforms": [
                        "darwin"
                    ]
                },
                {
                    "name": "@electron-forge/maker-dmg",
                    "config": {
                        "background": "./assets/dmg-background.png",
                        "format": "ULFO"
                    }
                },
                {
                    "name": "@electron-forge/maker-deb",
                    "config": {
                        "options": {
                            "maintainer": "Lucio Tarantino",
                            "homepage": "https://github.com/dianlight"
                        }
                    }
                },
                {
                    "name": "@electron-forge/maker-rpm",
                    "config": {
                        "options": {
                            "maintainer": "Lucio Tarantino",
                            "homepage": "https://github.com/dianlight"
                        }
                    }
                }
            ],
            "plugins": [
                [
                    "@electron-forge/plugin-webpack",
                    {
                        "mainConfig": "./webpack.main.config.js",
                        "renderer": {
                            "config": "./webpack.renderer.config.js",
                            "entryPoints": [{
                                "html": "./src/index.html",
                                "js": "./src/renderer.ts",
                                "name": "main_window"
                            }]
                        }
                    }
                ]
            ]
        }
    },
    "devDependencies": {
        "@babel/types": "^7.12.12",
        "@electron-forge/cli": "^6.0.0-beta.54",
        "@electron-forge/maker-deb": "^6.0.0-beta.54",
        "@electron-forge/maker-dmg": "^6.0.0-beta.54",
        "@electron-forge/maker-rpm": "^6.0.0-beta.54",
        "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
        "@electron-forge/maker-zip": "^6.0.0-beta.54",
        "@electron-forge/plugin-auto-unpack-natives": "^6.0.0-beta.54",
        "@electron-forge/plugin-webpack": "^6.0.0-beta.54",
        "@electron-forge/publisher-github": "^6.0.0-beta.54",
        "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
        "@types/adm-zip": "^0.4.33",
        "@types/colornames": "^1.1.1",
        "@types/d3": "^6.2.0",
        "@types/geojson": "^7946.0.7",
        "@types/jsts": "^0.17.3",
        "@types/readable-stream": "^2.3.9",
        "@types/serialport": "^8.0.1",
        "@types/temp": "^0.8.34",
        "@types/webpack-env": "^1.16.0",
        "@types/yaml": "^1.9.7",
        "@typescript-eslint/eslint-plugin": "^4.12.0",
        "@typescript-eslint/parser": "^4.12.0",
        "commitizen": "^4.2.2",
        "concurrently": "^5.3.0",
        "copy-webpack-plugin": "^6.4.1",
        "css-loader": "^5.0.1",
        "cz-conventional-changelog": "^3.3.0",
        "electron": "^11.1.1",
        "electron-license": "^1.1.0",
        "eslint": "^7.17.0",
        "eslint-plugin-import": "^2.22.1",
        "file-loader": "^6.2.0",
        "fork-ts-checker-webpack-plugin": "^6.0.8",
        "generate-changelog": "^1.8.0",
        "husky": "^5.0.6",
        "json-schema-to-typescript": "^10.1.1",
        "node-gyp": "^7.1.2",
        "node-loader": "^1.0.2",
        "npm-watch": "^0.7.0",
        "patch-package": "^6.2.2",
        "renamer": "^2.0.1",
        "style-loader": "^2.0.0",
        "threads-plugin": "^1.4.0",
        "ts-loader": "^8.0.14",
        "tsconfig-paths-webpack-plugin": "^3.3.0",
        "type-fest": "^0.20.2",
        "typescript": "^4.1.3",
        "url-loader": "^4.1.1",
        "vue-devtools": "^5.1.4",
        "vue-i18n-extract": "^1.1.10",
        "vue-loader": "^15.9.6",
        "vue-template-compiler": "^2.6.12"
    },
    "dependencies": {
        "@ncform/ncform": "https://github.com/dianlight/ncform/releases/download/v1.7.8-i18n/ncform-v1.7.8-i18n.tar.gz",
        "@ncform/ncform-common": "https://github.com/dianlight/ncform/releases/download/v1.7.8-i18n/ncform-common-v1.7.8-i18n.tar.gz",
        "@ncform/ncform-theme-elementui": "https://github.com/dianlight/ncform/releases/download/v1.7.8-i18n/ncform-theme-elementui-v1.7.8-i18n.tar.gz",
        "@svgdotjs/svg.js": "^3.0.16",
        "@types/concaveman": "^1.1.3",
        "adm-zip": "^0.5.1",
        "axios": "^0.21.1",
        "colornames": "^1.1.1",
        "concaveman": "^1.2.0",
        "conf": "^7.1.2",
        "electron-log": "^4.3.1",
        "electron-squirrel-startup": "^1.0.0",
        "electron-store": "^6.0.1",
        "element-ui": "^2.14.1",
        "gcode-toolpath": "^2.2.0",
        "geojson": "^0.5.0",
        "geojson-to-svg": "^1.1.3",
        "gerber-parser": "^4.2.4",
        "gerber-plotter": "^4.2.5",
        "gerber-to-svg": "^4.2.5",
        "highlight.js": "^10.5.0",
        "jsts": "^2.6.1",
        "lzma-native": "^6.0.1",
        "makerjs": "^0.17.0",
        "panzoom": "^9.4.1",
        "pcb-stackup": "^4.2.5",
        "readable-stream": "^3.6.0",
        "serialport": "^9.0.4",
        "stream": "^0.0.2",
        "temp": "^0.9.4",
        "threads": "^1.6.3",
        "three": "^0.124.0",
        "tightcnc": "^0.2.0",
        "typed-web-workers": "^4.0.0",
        "update-electron-app": "^2.0.1",
        "vue": "^2.6.12",
        "vue-class-component": "^7.2.6",
        "vue-i18n": "^8.22.2",
        "vue-property-decorator": "^9.1.2",
        "vue-router": "^3.4.9",
        "vuex": "^3.6.0",
        "vuex-map-fields": "^1.4.1",
        "vuex-persist": "^3.1.3",
        "yaml": "^1.10.0"
    }
}